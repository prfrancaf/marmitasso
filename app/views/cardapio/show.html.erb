<p id="notice"><%= notice %></p>

<h1>Pratos</h1>

<table>
  <thead>
  <tr>
    <th>Nome</th>
    <th>Descricao</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @pratos.each do |prato| %>
      <tr>
        <td><%= prato.nome %></td>
        <td><%= prato.descricao %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>
